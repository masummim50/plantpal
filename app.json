{
  "expo": {
    "name": "Garden Book",
    "slug": "gardeners",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gardenicon.png",
    "scheme": "gardeners",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gardenicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.masummim50.gardeners"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gardenicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#DBFFEE"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "413a6f21-877f-4812-b5a0-335360fe48ec"
      }
    }
  }
}
